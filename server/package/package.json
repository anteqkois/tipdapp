{
  "name": "@tipdapp/server",
  "author": "Antek Kois",
  "license": "MIT",
  "version": "0.0.3",
  "description": "Types from Prisma for models and other shared code to my web3 app",
  "main": "lib/cjs/package.js",
  "module": "lib/esm/package.js",
  "types": "lib/esm/package.d.ts",
  "files": [
    "lib",
    "package.json",
    "schema.prisma",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/package.d.ts",
        "default": "./lib/esm/package.js"
      },
      "require": {
        "types": "./lib/cjs/package.d.ts",
        "default": "./lib/cjs/package.js"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anteqkois/tipdapp.git",
    "directory": "server"
  },
  "bugs": {
    "url": "https://github.com/anteqkois/tipdapp/issues"
  },
  "homepage": "https://github.com/anteqkois/tipdapp/tree/master/server/package",
  "keywords": [
    "prisma",
    "Prisma",
    "typescript",
    "TypeScript",
    "type",
    "types",
    "esbuild",
    "microservice",
    "share"
  ],
  "scripts": {
    "prebuild": "rm -rf lib",
    "build": "npx prisma generate && npm run bundle && npm run build:ts && cp ../prisma/schema.prisma .",
    "build:local": "npm run build && cp -r ./lib ../../dapp/node_modules/@tipdapp/server/ && cp ./{schema.prisma,package.json,README.md} ../../dapp/node_modules/@tipdapp/server/",
    "bundle": "node esbuild.config.js",
    "prepack": "npm run build",
    "publishPackage": "npm version patch && npm publish",
    "postinstall": "npx prisma generate",
    "build:ts": "tsc --build tsconfig.esm.json && tsc --build tsconfig.cjs.json"
  },
  "dependencies": {
    "@prisma/client": "^4.8.0"
  },
  "devDependencies": {
    "esbuild": "^0.15.18",
    "prisma": "^4.8.0",
    "tsc": "^2.0.4",
    "typescript": "^4.9.4"
  }
}
