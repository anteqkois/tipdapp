{
  "name": "@tipdapp/api-gateway",
  "version": "1.0.0",
  "description": "API Gateway for my frontend client to handle all routes from different microservices",
  "main": "index.ts",
  "scripts": {
    "dev": "NODE_ENV=development dotenv_config_path=.env.development nodemon",
    "prebuild": "rm -rf lib",
    "build": "npm run bundle && npm run build:ts",
    "bundle": "node esbuild.config.js",
    "lint": "npx eslint .",
    "prepack": "npm run build",
    "publishPackage": "npm version patch && npm publish",
    "build:ts": "tsc --build tsconfig.esm.json && tsc --build tsconfig.cjs.json"
  },
  "keywords": [
    "Api",
    "api",
    "gateway",
    "apigateway",
    "express"
  ],
  "author": "Antek Kois",
  "license": "ISC",
  "dependencies": {
    "@tipdapp/database": "*",
    "@tipdapp/server": "^0.0.0",
    "axios": "^1.3.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "eslint": "^8.34.0",
    "eslint-config-tipdapp-node": "^0.0.0",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "nodemon": "^2.0.20",
    "redis": "^4.6.4",
    "siwe": "^2.1.3",
    "typescript": "^4.9.5",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@tipdapp/tsconfig": "^0.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@types/jsonwebtoken": "^9.0.1",
    "esbuild": "^0.17.5"
  }
}
