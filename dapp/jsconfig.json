{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/src/*": ["src/*"],
      "@/components/*": ["src/components/*"],
      "@/api/*": ["src/api/*"],
      "@/utils/*": ["src/utils/*"],
      "@/lib/*": ["src/lib/*"],
      "@/schema/*": ["src/schema/*"],
      "@/hooks": ["src/hooks"],
      "@/hooks/*": ["src/hooks/*"],
      // "@/lib": ["src/lib"],
    }
  },
  "include": [
    "**/*.js",
    "**/*.jsx",
    "../contracts/utils/packOracleData.js",
    "src/pages/api/auth/[...nextauth].ts",
    "lib/prismaClient.js"
  ],
  "exclude": ["node_modules", "**/node_modules", "dist"]
}
