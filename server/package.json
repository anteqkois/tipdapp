{
  "name": "qoistip",
  "version": "0.0.2",
  "engines ": {
    "node": "18.12.0"
  },
  "description": "frontend and backend to qoisdapp",
  "main": "server.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anteqkois/qoisdapp.git"
  },
  "author": "Antek Kois",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anteqkois/qoisdapp/issues"
  },
  "homepage": "https://github.com/anteqkois/qoisdapp#readme",
  "scripts": {
    "start": "NODE_ENV=production node dist/server.js",
    "build": "npx tsc",
    "dev": "NODE_ENV=development dotenv_config_path=.env.development nodemon",
    "dev:update": "npm run prisma:update && npm run dev",
    "prelib": "npx prisma generate",
    "lib": "npm run lib:build && npm run lib:link && npm run lib:copy",
    "lib:build": "rm -rf dist/package && tsc --build tsconfig.es6.json && tsc --build tsconfig.es5.json",
    "lib:link": "cd dist && npm link",
    "lib:copy": "for f in index.js:prisma.js index.d.ts:prisma.d.ts; do IN=$(echo $f | cut -d ':' -f1) OUT=$(echo $f | cut -d ':' -f2) && cp ./node_modules/.prisma/client/$IN ./dist/lib/es6/src/types/$OUT; done",
    "prisma:generate": "npx prisma generate && for f in index.js:prisma.js index.d.ts:prisma.d.ts; do IN=$(echo $f | cut -d ':' -f1) OUT=$(echo $f | cut -d ':' -f2) && cp ./node_modules/.prisma/client/$IN ./src/types/$OUT; done",
    "prisma:update": "npx prisma db push && ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "axios": "^0.26.0",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "siwe": "^2.1.3",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.8.3",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "ethers": "^5.7.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "prisma": "^4.3.1",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4",
    "typescript": "^4.9.3"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
