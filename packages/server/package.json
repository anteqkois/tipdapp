  "license": "MIT",
  "version": "0.0.0",
  "description": "Node server utils for other node microservices",
  "main": "index.ts",
  "types": "index.ts",
  "files": [
    "lib/*",
    "index.ts",
    "package.json"
  ],
  "keywords": [
    "typescript",
    "TypeScript",
    "type",
    "types",
    "microservice",
    "share",
