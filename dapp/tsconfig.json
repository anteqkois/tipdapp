{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/modules/*": ["src/modules/*"],
      "@/shared/*": ["src/shared/*"],
      "@/assets/*": ["src/assets/*"],
      "@/types/*": ["src/types/*"],
      "@/types": ["src/types"],
      "@/api/*": ["src/api/*"],
      "@/utils/*": ["src/utils/*"],
      "@/utils": ["src/utils"],
      "@/lib/*": ["src/lib/*"],
      "@/lib": ["src/lib"],
      "@/validation/*": ["src/validationOld/*"]
    },
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "preserveSymlinks": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/shared/TipdappContracts/types/index.d.ts"
  ],
  "exclude": [
    "node_modules",
    "**/node_modules/*",
    ".next",
    "dist",
    "src/artifacts",
  ]
}
