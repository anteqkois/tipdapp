    "dotenv": "^16.0.3",
    "esbuild": "^0.17.8",
    "eslint-config-tipdapp-node": "^0.0.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.20.6"
