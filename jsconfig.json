{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/components/*": ["src/components/*"],
            "@/services/*": ["src/services/*"],
            "@/hooks/*": ["src/hooks/*"],
            "@/utils/*": ["src/utils/*"]
        }
    },
    "include": ["src/**/*.js", "src/**/*.jsx"],
    "exclude": ["node_modules", "**/node_modules", "dist"]
}
