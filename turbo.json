{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["@tipdapp/contracts#compile"]
    },
    "lint": {
      "cache": false,
      "dependsOn": ["^lint"]
    },
    "docker": {
      "cache": false,
      "dependsOn": ["//#docker:dev"]
    },
    "//#docker:dev": {
      "cache": false
    },
    "@tipdapp/contracts#compile": {
      "cache": false
    }
  }
}
