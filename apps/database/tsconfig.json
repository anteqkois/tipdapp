{
  "extends": "@tipdapp/tsconfig/tsconfig.node.json",
  "compilerOptions": {
    "paths": {
      "@config/*": ["./src/config/*"],
      "@services/*": ["./src/services/*"],
      "@types/*": ["./src/types/*"],
      "@types": ["./src/types"]
    }
  },
  // "include": ["src/"],
  "include": ["server.ts", "src/**/*.ts", "src/**/*.d.ts"],
  "exclude": [
    "node_modules",
    "**/node_modules/*",
    "./.log",
    "dist",
    "prisma.d.ts"
  ]
}
