{
  "name": "@tipdapp/database",
  "version": "0.0.7",
  "engines ": {
    "node": "18.12.0"
  },
  "description": "Main server to handel database layer for tipdapp dapp and types from Prisma for models and related types.",
  "main": "lib/cjs/package.js",
  "module": "lib/esm/package.js",
  "types": "lib/esm/package.d.ts",
  "files": [
    "lib",
    "index.json",
    "prisma/schema.prisma",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/index.d.ts",
        "default": "./lib/esm/index.js"
      },
      "require": {
        "types": "./lib/cjs/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anteqkois/tipdapp.git"
  },
  "author": "Antek Kois",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anteqkois/tipdapp/issues"
  },
  "homepage": "https://github.com/anteqkois/tipdapp#readme",
  "scripts": {
    "prebuild": "rm -rf lib",
    "build": "npx prisma generate && npm run bundle && npm run build:ts",
    "postbuild": "npx prisma generate",
    "build:ts": "tsc --build tsconfig.esm.json && tsc --build tsconfig.cjs.json",
    "dev": "NODE_ENV=development dotenv_config_path=.env.development nodemon",
    "dev:update": "npm run db:update && npm run dev",
    "db:update": "npx prisma db push && ts-node prisma/seed.ts",
    "bundle": "node esbuild.config.js",
    "lint": "eslint lint",
    "prepack": "npm run build",
    "publishPackage": "npm version patch && npm publish",
    "postinstall": "npx prisma generate",
    "start": "NODE_ENV=production node dist/server.js"
  },
  "dependencies": {
    "@prisma/client": "^4.7.1",
    "amqplib": "^0.10.3",
    "axios": "^1.3.2",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "express": "^4.18.2",
    "express-query-parser": "^1.3.3",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "siwe": "^2.1.3",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@types/amqplib": "^0.10.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.39.0",
    "esbuild": "^0.17.6",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-tipdapp-next": "^0.0.0",
    "eslint-config-tipdapp-node": "*",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "prisma": "^4.7.1",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4",
    "typescript": "^4.9.5"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "keywords": [
    "prisma",
    "Prisma",
    "typescript",
    "TypeScript",
    "type",
    "types",
    "esbuild",
    "microservice",
    "share"
  ]
}
